algoritmo "aaaaaa"
var
  s: vetor[1..10] de real
  i: inteiro
inicio
  para i de 1 ate 10 faca
    escreval("Digite o valores")
    leia(s[i])
  fimpara
  limpatela
  para i de 1 ate 10 faca
escreva(s[i])
fimpara
escreval
  para i de 1 ate 10 faca
s[i]<-s[i]/2
   escreva(s[i])
  fimpara
  
fimalgoritmo


