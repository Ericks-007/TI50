algoritmo "Algoritmo20"

var
   A, B : inteiro
inicio
   escreval("Determine qual numero é maior")
   escreval("numero A")
   leia(A)
   escreval("numero B")
   leia(B)
   
   se (A > B) entao
   escreval("numero",A," é maior que",B)
   senao
   se (A < B) entao
   escreval("numero",B," é maior que",A)
   senao
   escreval("A e B são iguais")
   fimse
   fimse
fimalgoritmo
