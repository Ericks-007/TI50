algoritmo "aaaaaa"
var
  s: vetor[1..15] de inteiro
  i: inteiro
inicio
  para i de 1 ate 15 faca
    escreval("Digite o valores")
    leia(s[i])
  fimpara
  limpatela
  para i de 1 ate 15 faca
escreva(s[i])
fimpara
escreval
  para i de 1 ate 15 faca
  se s[i] >=10 entao
  escreval ("Valor => 10", s[i])
  fimse
  fimpara
fimalgoritmo

