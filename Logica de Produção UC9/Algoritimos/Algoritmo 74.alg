algoritmo "Algoritmo 74"
var
   v: vetor[1..10] de inteiro
   i : inteiro
inicio
   escreval("Vetor de 10 elementos inteiros")
     para i de 1 ate 10 faca
      escreval ("Digite v[",i,"]: ")
      leia (v[i])
     fimpara
    limpatela
    escreval("Vetor v, de elementos, na tela:")
     para i de 1 ate 10 faca
        escreva(v[i]," ")
     fimpara
fimalgoritmo
